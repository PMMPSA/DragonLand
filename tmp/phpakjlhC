    /** @var bool */
    protected $mode;

    /**
     * @param BaseAPI $api
     * @param Player $player
     * @param bool $mode
     */
    public function __construct(BaseAPI $api, Player $player, bool $mode){
        parent::__construct($api);
        $this->player = $player;
        $this->isFlying = $api->canFly($player);
        $this->mode = $mode;
    }

    /**
     * The player to work over
     *
     * @return Player
     */
